<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        layout="@layout/standard_title_bar"
        />
    <include
        layout="@layout/view_non_data"
        />

    <com.example.project.limolive.widget.AutoSwipeRefreshLayout
        android:id="@+id/swipe_cart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/rl_calculator_money"
        >
        <ListView
            android:id="@+id/lv_my_collect_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/background1"
            android:dividerHeight="@dimen/activity_padding_margin1"
            />
    </com.example.project.limolive.widget.AutoSwipeRefreshLayout>






</LinearLayout>